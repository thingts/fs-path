{
  "name": "@thingts/fs-path",
  "version": "1.2.4",
  "description": "Type-safe, ergonomic package for working with paths and files in Node.js",
  "keywords": [
    "copy",
    "file",
    "filename",
    "filepath",
    "filesystem",
    "fs",
    "glob",
    "mkdir",
    "node",
    "nodejs",
    "path",
    "posix",
    "rename",
    "tempdir",
    "tempfile",
    "touch",
    "typescript"
  ],
  "homepage": "https://github.com/thingts/fs-path#readme",
  "bugs": {
    "url": "https://github.com/thingts/fs-path/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thingts/fs-path.git"
  },
  "license": "MIT",
  "author": "ronen barzel",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "docs": "typedoc",
    "lint": "tsc --build --noEmit ; eslint ; knip",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "knip": "^5.71.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typedoc-github-theme": "^0.3.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@thingts/path": "^2.0.2",
    "fast-glob": "^3.3.3"
  }
}
